    "tokenColors": [
        {
            "name": "Base",
            "scope": [
                "source",
                "meta.paragraph.markdown"
            ],
            "settings": {
                "foreground": "#ffffff",
            }
        },
        {
            "name": "Keywords",
            "scope": "keyword.other",
            "settings": {
                "foreground": "#6c8a8b",
            }
        },
        {
            "name": "CSS Classes",
            "scope": "entity.name.tag.css",
